
/* ------------------------- */
/* POPUP */
/* ------------------------- */

.overlay {
	background: rgba(0,0,0,.4);
	position: fixed;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	align-items: center;
	justify-content: center;
	display: flex;
	visibility: hidden;
}

.overlay.active {
	visibility: visible;
}

.popup {
	background: #F8F8F8;
	box-shadow: 0px 0px 5px 0px rgba(0,0,0,0.3);
	border-radius: 50px;
	font-family: 'Montserrat', sans-serif;
	text-align: center;
	width: 600px;
	transition: .3s ease all;
	height: 200px;
	
}

.icon-popup{
	transform: scale(0.5);
}

.popup .btn-close-popup {
	font-size: 40px;
	line-height: 42px;
	display: block;
	text-align: right;
	transition: .3s ease all;
	color: #BBBBBB;
	cursor: pointer;
	padding:0 18 0;
	background: #0c63cb;
	border-radius: 30px;
}

.popup .btn-close-popup:hover {
	color: rgb(255, 255, 0)x#000;
	color: gold;
}



/* ------------------------- */
/* ANIMACIONES */
/* ------------------------- */
.popup.active {	transform: scale(1); opacity: 1; border-radius: 20px;}
.popup.active h3 { animation: entradaTitulo .5s ease .3s forwards;height: 0px; }
.popup.active h4 { animation: entradaSubtitulo .5s ease .3s forwards;font-size: large;margin: 0 0 0 0;}
.popup.active .contenedor-inputs { animation: entradaInputs 3s linear 3s forwards; }

@keyframes entradaTitulo {
	from {
		opacity: 0;
		transform: translateY(-25px);
	}

	to {
		transform: translateY(0);
		opacity: 1;
	}
}

@keyframes entradaSubtitulo {
	from {
		opacity: 0;
		transform: translateY(25px);
	}

	to {
		transform: translateY(0);
		opacity: 1;
	}
}

