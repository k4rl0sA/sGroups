
  /*=========ZONE FILTERS AND DATA=============*/
.wrapper{
    max-width: 100%;
    width: 90%;
    height: max-content;
    padding: 15px 20px;
    border-radius: 5px;
    overflow: hidden;
    background: var(--primary-color-light);
    box-shadow: 0px 5px 15px rgba(0,0,0,0.1);
  }

  .top-menu{
    background:#0c63cb;
    border-radius: 14px 14px 0 0;
  }
   
  .wrapper header{
    font-size: 30px;
    font-weight: 600;
    padding-bottom: 20px;
  }
  .wrapper nav{
    position: relative;
    width: 50%;
    display: flex;
    align-items: center;
    left:8px;
  }

  .wrapper nav label{
    font-family: "Merienda", cursive;
    display: block;
    height: 100%;
    width: 100%;
    text-align: center;
    line-height: 50px;
    cursor: pointer;
    position: relative;
    top:8px;
    z-index: 1;
    color: var(--color-icons);
    font-size: 17px;
    border-radius: 5px;
    margin: 0 5px;
    transition: all 0.3s ease;
  }
  /*.wrapper nav label:hover{
     background: var(--body-color);
    color: var(--text-color); 
  }*/
  #filtros:checked ~ nav label.filtros,
  #datos:checked ~ nav label.datos{
    color: var(--color-icons);
  }
  nav label i{
    padding-right: 7px;
  }
  nav .slider{
    position: absolute;
    height: 100%;
    width: 50%;
    top:8px;
    left: 0;
    bottom: 0;
    z-index: 0;
    border-radius: 5px;
    background: var(--primary-color);
    transition: all 0.3s ease;
  }
  input[type="radio"]{
    display: none;
  }
  #datos:checked ~ nav .slider{
    left: 50%;
  }
  section .content{
    display: none;
    background: var(--primary-color-light);
    color: var(--text-color);
  }
  #filtros:checked ~ section .content-1,
  #datos:checked ~ section .content-2{
    display: block;
  }
.sidebar.close .wrapper nav label{
    font-size: 10px;
}
  section .content .title{
    font-family: "Poiret One", sans-serif;
    font-size: 21px;
    font-weight: 500;
    margin: 15px 0 10px 0;
  }
  section .content p{
  text-align: justify;
  }
