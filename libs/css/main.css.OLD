/*===========FORMS=================*/ 
.input-box {
  position: relative;
  width: 100%;
  margin-bottom: 20px;
}

.input-box input[type="text"],
.input-box input[type="number"],
.input-box input[type="date"],
.input-box select,
.input-box textarea {
  padding: 15px 15px 3px 15px;
  border-radius: 5px;
  border: none;
  border-bottom: 2px solid #ccc;
  background-color: transparent;
  font-size: 16px;
  outline: none;
  transition: border-bottom var(--tran-03);
}

.input-box label {
  text-transform: capitalize;
  position: absolute;
  left: 10px;
  top: 50%;
  transform: translateY(-50%);
  color: var(--text-color);
  font-size: 16px;
  pointer-events: none;
  transition: all var(--tran-03);
}

.input-box select + label {
  left: 8em;
  top: 61%;
}

.input-box input:focus ~ label,
.input-box input:disabled ~ label,
.input-box input:valid ~ label{
  text-transform: capitalize;
  top: -5px;
  font-size: 16px;
  color: var(--primary-color);
}

.input-box select:focus ~ label,
.input-box select:disabled ~ label,
.input-box select:valid ~ label{
  text-transform: capitalize;
  top: -5px;
  left: 10px;
  font-size: 16px;
  color: var(--primary-color);
}

.input-box textarea:focus ~ label,
.input-box textarea:valid ~ label {
  text-transform: capitalize;
  top: -5px;
  font-size: 16px;
  color:var(--primary-color);
}

.input-box input:not(:placeholder-shown),
.input-box select:not(:placeholder-shown),
.input-box textarea:not(:placeholder-shown) {
  border-bottom: 3px solid var(--primary-color);
}

.input-box input:not(:focus):not(:disabled),
.input-box select:not(:focus):not(:disabled),
.input-box textarea:not(:focus):not(:disabled) {
  border-bottom: 3px solid var(--primary-color);
  animation: fillBorder 1s ease forwards;
}




@keyframes fillBorder {
  0% {
    width: 0;
    right: 50%;
    transform-origin: right;
  }
  100% {
    width: 100%;
    right: 0;
  }
}

.form-cmp {
  width: 90%;
  margin: 0 auto;
  padding: 20px;
  border-radius: 5px;
  background-color: var(--body-color);
  box-shadow: 0 0 25px rgba(0, 0, 0, 0.1);
}

.btn {
  width: 100%;
  height: 45px;
  background: var(--primary-color);
  border: none;
  border-radius: 40px;
  cursor: pointer;
  font-size: 16px;
  color: #fff;
  font-weight: 600;
  z-index: 1;
  overflow: hidden;
  position: relative;
  transition: background-color var(--tran-03);
}

.btn::before {
  content: '';
  position: absolute;
  top: -100%;
  left: 0;
  width: 100%;
  height: 300%;
  background: linear-gradient(var(--primary-color), var(--secundary-color));
  z-index: -1;
  transition: .5s;
}

.btn:hover::before {
  top: 0;
}

.btn:hover {
  background-color: var(--secundary-color);
}

button.btn[disabled] {
  background-color: #ccc;
  cursor: not-allowed;
}
 
.error {
  padding: 20px;
  background-color: var(--error-color);
  color: white;
  border-radius: 25px;
  position: absolute;
  z-index: 100;
  top: 0;
}

.ok {
  padding: 20px;
  background-color: #00f372d6;
  color: white;
  border-radius: 25px;
  position: absolute;
  z-index: 100;
  top: 0;
}

